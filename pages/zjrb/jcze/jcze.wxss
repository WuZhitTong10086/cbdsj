.list {
  min-height: 100vh;
  width: 100%;
  padding: 0rpx 15rpx 0rpx 29rpx;
  box-sizing: border-box;
  top: 170rpx;
  position: fixed;
}

.table {
  overflow: hidden;
  overflow-x: auto;
  width: 100%;
  white-space: nowrap;
  background-color: #fff;
}

.thead {
  width: 100%;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 500;
  color: rgba(81, 81, 81, 1);
  line-height: 32rpx;
  background-color: #fff;
  white-space: nowrap;
  height: 62rpx;
  background-color: #F5F5F5;
}

.thead .th {
  display: inline-block;
  width: 80rpx;
  /* margin-left: 4rpx; */
  position: relative;
  height: 66rpx;
  line-height: 66rpx;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: bold;
  vertical-align: middle;
  /* padding-left: 10rpx; */
  /* padding-right: 10rpx; */
}


.thead .th text {
  display: block-inline;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: bold;
}

.tbody .main {
  white-space: nowrap;
  background-color: #fff;
}

