  font-size: 23rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  color: rgba(81, 81, 81, 1);
  height: 54rpx;
  line-height: 54rpx;
  display: inline-block;
  /* overflow: hidden; */
}

.tr:nth-child(even){
  background-color: #F5F5F5;
  
}

/* 过长显示省略号 */
.dot {
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}